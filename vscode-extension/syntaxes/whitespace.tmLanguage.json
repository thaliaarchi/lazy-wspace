{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Whitespace",
  "scopeName": "source.ws",
  "patterns": [
    {
      "name": "whitespace.space.ws",
      "match": " "
    },
    {
      "name": "whitespace.tab.ws",
      "match": "\t"
    }
  ],
  "fileTypes": ["ws"]
}
