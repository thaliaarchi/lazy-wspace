{
  "autoCloseBefore": ");=\\s",
  "autoClosingPairs": [
    {
      "open": "(",
      "close": ")"
    },
    {
      "open": "\"",
      "close": "\"",
      "notIn": ["string"]
    },
    {
      "open": "'",
      "close": "'",
      "notIn": ["string"]
    }
  ],
  "brackets": [
    ["(", ")"]
  ],
  "colorizedBracketPairs": [
    ["(", ")"]
  ],
  "comments": {
    "lineComment": ";"
  },
  "folding": {
    "offSide": true
  },
  "indentationRules": {},
  "onEnterRules": [],
  "surroundingPairs": [
    ["(", ")"]
  ],
  "wordPattern": "\\b[0-9a-zA-Z~!@#$%^&*\\-_=+|:,<.>/?]+\\b"
}
